<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="20dp"
    card_view:cardCornerRadius="4dp">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

<TextView
android:id="@+id/doctorplan_fidlabel"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_alignParentTop="true"
android:text="Doctorplan_fid:"
android:textColor="#333"
android:layout_alignParentRight="true"
/>
<TextView
android:id="@+id/doctorplan_fid"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_alignParentTop="true"
android:text="Doctorplan_fid"
android:textColor="#333"
android:layout_centerHorizontal="true"
android:layout_toLeftOf="@id/doctorplan_fidlabel"
/>
<TextView
android:id="@+id/financial_transaction_fidlabel"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/doctorplan_fidlabel"
android:text="Financial_transaction_fid:"
android:textColor="#333"
android:layout_alignParentRight="true"
/>
<TextView
android:id="@+id/financial_transaction_fid"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/doctorplan_fidlabel"
android:text="Financial_transaction_fid"
android:textColor="#333"
android:layout_centerHorizontal="true"
android:layout_toLeftOf="@id/financial_transaction_fidlabel"
/>
<TextView
android:id="@+id/presencetype_fidlabel"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/financial_transaction_fidlabel"
android:text="Presencetype_fid:"
android:textColor="#333"
android:layout_alignParentRight="true"
/>
<TextView
android:id="@+id/presencetype_fid"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/financial_transaction_fidlabel"
android:text="Presencetype_fid"
android:textColor="#333"
android:layout_centerHorizontal="true"
android:layout_toLeftOf="@id/presencetype_fidlabel"
/>
<TextView
android:id="@+id/reserve_datelabel"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/presencetype_fidlabel"
android:text="Reserve_date:"
android:textColor="#333"
android:layout_alignParentRight="true"
/>
<TextView
android:id="@+id/reserve_date"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/presencetype_fidlabel"
android:text="Reserve_date"
android:textColor="#333"
android:layout_centerHorizontal="true"
android:layout_toLeftOf="@id/reserve_datelabel"
/>
<TextView
android:id="@+id/role_systemuser_fidlabel"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/reserve_datelabel"
android:text="Role_systemuser_fid:"
android:textColor="#333"
android:layout_alignParentRight="true"
/>
<TextView
android:id="@+id/role_systemuser_fid"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_margin="@dimen/text_margin"
android:textAppearance="?attr/textAppearanceListItem"
android:layout_below="@id/reserve_datelabel"
android:text="Role_systemuser_fid"
android:textColor="#333"
android:layout_centerHorizontal="true"
android:layout_toLeftOf="@id/role_systemuser_fidlabel"
/>        
</RelativeLayout>
</android.support.v7.widget.CardView>